{
  "stateMachine": {
    "id": "dockerScript",
    "initial": "mongodb",
    "context": {},
    "states": {
      "mongodb": {
        "entry": [
          {
            "type": "createContainer",
            "containerName": "defboxlocalmongodb",
            "dockerFolderName": "mongodb",
            "networkName": "attacknetwork",
            "ExposedPorts": {"27017/tcp": {}},
            "PortBindings": { "27017/tcp": [{ "HostPort": "27017" }]},
            "sustain": true,
            "awaitStart": true
          }
        ],
        "on": {
          "NEXT": "targetMachine"
        }
      },
      "targetMachine": {
        "entry": [
          {
            "type": "createContainer",
            "containerName": "defboxlocaltarget",
            "dockerFolderName": "targetMachine",
            "networkName": "attacknetwork",
            "ExposedPorts": {"3005/tcp": {}},
            "PortBindings": { "3005/tcp": [{ "HostPort": "3005" }]},
            "sustain": true,
            "awaitStart": true
          }
        ],
        "on": {
          "NEXT": "hackerMachine"
        }
      },
      "hackerMachine": {
        "entry": [
          {
            "type": "createContainer",
            "containerName": "defboxlocalhacker",
            "dockerFolderName": "hackerMachine",
            "sustain": true,
            "networkName": "attacknetwork",
            "ExposedPorts": {"3042/tcp": {}},
            "PortBindings": { "3042/tcp": [{ "HostPort": "3042" }]}
          }
        ],
        "on": {
          "NEXT": "complete"
        }
      },
      "complete": {
        "type": "final"
      }
    }
  }

}